base=/Users/mason/Workspaces/github/pinpoint/plugins
for i in `ls`
do
	if [ -d $base/$i ];
	then
		echo "enter "$base/$i
		src=$base/$i/src
		main=$src/main
		test=$src/test

		if [ -d $main ];
		then
			echo "processing "$main
			java=$main/java
			if [ -d $java ];
			then
				echo "processing "$java
				mkdir -p $java/com/baidu/oped/apm
				mv $java/com.baidu.oped.apm/* $java/com/baidu/oped/apm/
				rm -rf $java/com/navercorp
			fi

			resources=$main/resources
			if [ -d $resources ];
			then
				echo "processing "$resources
				mkdir -p $resources/com/baidu/oped/apm
				mv $resources/com.baidu.oped.apm/* $resources/com/baidu/oped/apm/
				rm -rf $resources/com/navercorp
			fi
		fi

		if [ -d $test ];
		then
			echo "Processing "$test
			java=$test/java
			if [ -d $java ];
			then
				echo "processing "$java
				mkdir -p $java/com/baidu/oped/apm
				mv $java/com.baidu.oped.apm/* $java/com/baidu/oped/apm/
				rm -rf $java/com/navercorp
			fi

			resources=$test/resources
			if [ -d $resources ];
			then
				echo "processing "$resources
				mkdir -p $resources/com/baidu/oped/apm
				mv $resources/com.baidu.oped.apm/* $resources/com/baidu/oped/apm/
				rm -rf $resources/com/navercorp
			fi
		fi
	fi

done 